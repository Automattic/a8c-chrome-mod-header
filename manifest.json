{
    "manifest_version": 3,
    "name": "a8c Modify Header Request",
    "version": "1.0.2",
    "description": "This is a simple Chrome extension that allows you to modify the headers of a request.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "declarativeNetRequest",
        "activeTab",
        "storage"
    ],
    "action": {},
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}